{
  "name": "xml-cdata-xslt-highlighting",
  "displayName": "XML CDATA XSLT Highlighting",
  "description": "Adds XSLT syntax highlighting to CDATA sections in XML files.",
  "version": "1.0.0",
  "publisher": "ChristinaRau",
  "engines": {
    "vscode": "^1.80.0"
  },
  "icon": "logo.png",
  "categories": ["Programming Languages"],
  "contributes": {
    "grammars": [
      {
        "scopeName": "source.xml.injection",
        "path": "./syntaxes/xml-cdata-xslt.json",
        "injectTo": ["text.xml", "source.xml"],
        "embeddedLanguages": {
          "string.unquoted.cdata.xml": "xsl"
        },
        "tokenTypes": {
          "string.unquoted.cdata.xml": "other"
        }
      }
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ChristinaRau/xml-cdata-xslt-highlighting.git"},
  "bugs": {
    "url": "https://github.com/ChristinaRau/xml-cdata-xslt-highlighting/issues"
  },
  "homepage": "https://github.com/ChristinaRau/xml-cdata-xslt-highlighting",
  "keywords": ["xml", "xslt", "cdata", "syntax highlighting"],
  "devDependencies": {
    "vsce": "^2.10.0"
  },
  "scripts": {
    "package": "vsce package",
    "publish": "vsce publish"
  },
  "license": "MIT"
}
